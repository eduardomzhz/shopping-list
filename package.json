{
  "name": "shopping-list",
  "description": "A simple shopping list app.",
  "version": "0.1.0",
  "author": "Eduardo Mart√≠nez <eduardo.mzhz@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "vue": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "android": "cordova build android && mv platforms/android/app/build/outputs/apk/debug/app-debug.apk $npm_package_name.apk",
    "build": "npm run vue && npm run android"
  },
  "dependencies": {
    "cordova-android": "^8.1.0",
    "vue": "^2.5.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cordova-plugin-whitelist": "^1.3.4",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "html-webpack-plugin": "^3.2.0",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "android"
    ]
  }
}